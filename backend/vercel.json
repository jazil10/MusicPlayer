{
  "version": 2,
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node",
      "config": {
        "memory": 1024,
        "maxDuration": 10
      }
    }
  ],
  "routes": [
    {
      "src": "/health",
      "dest": "server.js"
    },
    {
      "src": "/api/search",
      "dest": "server.js"
    },
    {
      "src": "/api/audio/(.*)",
      "dest": "server.js"
    }
  ]
} 